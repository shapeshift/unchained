{
  "version": "9.3.0",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "message": "chore(release): publish %v",
      "ignoreChanges": [
        "**/*.md"
      ]
    }
  },
  "packages": [
    "monitoring/*",
    "pulumi/*",
    "node/packages/*",
    "node/coinstacks/**/*",
    "node/pulumi/*",
    "go/coinstacks/**/pulumi/*",
    "go/pulumi/*"
  ]
}
